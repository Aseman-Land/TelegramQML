    QColor messageMediaNameColor rw;
    QColor messageMediaDateColor rw;
    bool messageMediaLightIcon rw = false;
    qreal messageRadius rw = 5;
    bool messageShadow rw = true;
    qreal messageShadowSize rw = 2;
    QColor messageShadowColor rw;
    qreal messagePointerHeight rw = 15;

    QColor headerColor rw;
    QColor headerTitleColor rw;
    QFont headerTitleFont rw;
    QColor headerDateColor rw;
    QFont headerDateFont rw;
    bool headerLightIcon rw = false;
    QColor headerSecretColor rw;
    QColor headerSecretTitleColor rw;
    QFont headerSecretTitleFont rw;
    QColor headerSecretDateColor rw;
    QFont headerSecretDateFont rw;
    bool headerSecretLightIcon rw = false;
    qreal headerHeight rw = 50;

    QColor panelColor rw;
    bool panelLightIcon rw = true;
    qreal panelPointerHeight rw = 12;
    QColor panelTooltipBackground rw;
    QColor panelTooltipTextColor rw;
    QColor panelShadowColor rw;
    qreal panelShadowWidth rw = 5;

    QColor sidebarColor rw;
    QColor sidebarFontColor rw;
    QFont sidebarFont rw;
    QColor sidebarPhoneBackground rw;
    QColor sidebarPhoneColor rw;
    QFont sidebarPhoneFont rw;
